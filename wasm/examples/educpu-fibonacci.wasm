#meta
name: "Fibonacci"
author: "Kai Tamkun"
orcid: "0000-0001-7405-6654"
version: "1"

#code

fibonacci()
<halt>

sub fibonacci() {
	$a0 -= 2
	1 -> $t0
	1 -> $t1
	0 -> $r0
	@fibonacci_loop
		: fibonacci_loop_done if $a0 == $0
		$a0--
		$t0 + $t1 -> $r0
		[ $r0
		$t1 -> $t0
		$r0 -> $t1
		: fibonacci_loop
	@fibonacci_loop_done
	!done
}
